            <% if (liked_playlists.length > 0) { %>
            <h2>Liked playlists</h2>
            <div class="card-grid card-grid-3">
              <% liked_playlists.forEach(item => { %> <%-
              include('./components/playlist-card',{item: item}) %> <% }) %>
            </div>
            <% } %>

            <h2>Playlists for you</h2>
            <div class="carousel">
              <div class="card-grid">
                <% playlists.forEach(item => { %> <%-
                include('./components/playlist-card',{item: item}) %> <% }) %>
              </div>
              
              <section class="placeholder-buttons">
                <button class="storiesButton" aria-label="button stories">stories</button>
                <button class="playlistsButton" aria-label="button playlists"> playlists </button>
                <a href="#">Show all</a>
              </section>


              <section class="list-stories">
                <% stories.forEach(story => {%>
                  <article class="li-story">
                    <div>
                      <img class="img-story" src="https://fdnd-agency.directus.app/assets/<%= story.image %>" alt="story image">
                    </div>
                    <div class="text-content-story">
                      <h1 class="title-story"><%= story.title %> </h1>
                      <p class="summaryStory "> 
                        <%= story.summary %>
                      </p>
                      <div class="interactions-story">
                        <img class="icon-playbutton" src="/images/play-button.svg" alt="play button icon">
                        <img class="icon-download" src="/images/cloud-download-outline.svg" alt="download icon">
                        <button class="btn-add">
                            <img class="icon-add" src="/images/106230_add_icon.svg" alt="add button">
                        </button>
          
                    </div>
                    </div>
                    
                    
                  </article>
                <% });  %>
              </section>
              <section class="list-playlists">
                <!-- dit moet nog met een if statement -->
                <p>No playlist yet</p>
                <article class="card-grid">
                  <% playlists.forEach(item => { %> <%-
                  include('./components/playlist-card',{item: item}) %> <% }) %>
                </article>
              </section>
            </div>
          </div>
        </div>
        
      </div>
